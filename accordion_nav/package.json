{
  "name": "jquery-reference",
  "version": "0.0.1",
  "description": "jQuery 학습을 위한 프로젝트",
  "scripts": {
    "dev": "node_modules/.bin/npm-run-all --parallel --continue-on-error pug sass babel serve",
    "serve": "node_modules/.bin/live-server --port=4001 --cors",
    "pug": "node_modules/.bin/pug source/pug -w --out dist --pretty",
    "sass": "node_modules/.bin/node-sass -wr --output-style compressed -o dist/css --source-map true --source-map-contents source/sass",
    "babel": "node_modules/.bin/babel -w source/es6 -d dist/js"
  },
  "dependencies": {
    "bulma": "^0.4.3",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "live-server": "^1.2.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "pug-cli": "^1.0.0-alpha6"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
